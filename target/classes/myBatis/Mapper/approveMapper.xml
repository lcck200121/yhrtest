<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yhr.Mapper.approveMapper">
    <select id="queryLeaveList" resultType="leaveApprove">
        Select * from leave_info
    </select>

    <insert id="addLeave" parameterType="leaveApprove">
    insert into leave_info(content,createrUser,firstTime,secondTime) values (#{content},#{create_User},#{firstTime},#{secondTime})
    </insert>

</mapper>
